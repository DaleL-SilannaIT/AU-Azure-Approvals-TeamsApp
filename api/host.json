{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "default": "Information",
      "Function": "Information",
      "Host.Results": "Information",
      "Host.Aggregator": "Information",
      "Host.Executor": "Information",
      "Host.Startup": "Information",
      "Host.General": "Information",
      "Host.Triggers": "Information",
      "Host.Bindings": "Information",
      "Host.Function": "Information",
      "Host.Function.Console": "Information",
      "Host.Function.Console.Log": "Information"
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  }
}